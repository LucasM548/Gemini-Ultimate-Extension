{
  "manifest_version": 3,
  "name": "Gemini Ultimate",
  "version": "1.1",
  "default_locale": "en",
  "description": "__MSG_extDescription__",
  "permissions": ["storage"],
  "host_permissions": [
    "https://gemini.google.com/*"
  ],
  "icons": {
    "16": "Icons/Logo.png",
    "48": "Icons/Logo.png",
    "128": "Icons/Logo.png"
  },
  "action": {
    "default_title": "Gemini Ultimate",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "Icons/Logo.png",
      "48": "Icons/Logo.png",
      "128": "Icons/Logo.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://gemini.google.com/*"],
      "js": ["config.js", "content.js"],
      "run_at": "document_end"
    }
  ]
}
